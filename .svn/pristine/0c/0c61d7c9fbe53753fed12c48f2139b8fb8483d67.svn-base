import axios from '@/libs/axios';

export const advanceSave = ({ data, nodeName }) => axios({
  url: '/bdc/fwdt/zxsq/saveYGDJ',
  method: 'post',
  baseUrlType: 'nres',
  params: {
    nodeName
  },
  data,
});

