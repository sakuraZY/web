import axios from '@/libs/axios';

/**
 * 初始化附件实例目录
 * @param {*} data 附件实例参数
 */
export const getFile = data => axios({
  url: '/doc/attachment/init',
  method: 'post',
  params: data,
});

/**
 * 初始化附件实例目录
 * @param {*} data 附件实例参数
 */
export const init = (params, binids) => axios({
  url: '/doc/attachment/init',
  method: 'post',
  params,
  data: binids,
});
