.geo-header{
  height: 125px;
  display: flex;
  justify-content: space-between;
  align-items: center;
  &-left{
    display: flex;
    align-items: center;
    .logo{
      width: 64px;
      height: 64px;
      margin-right: 25px;
      background: url('../../../assets/logo.png') center no-repeat;
    }
    .system-name{
      &-main{
        font-size: 32px;
        color: #000;
        font-weight: bold;
        margin-bottom: 9px;
      }
      &-sub{
        font-size: 20px;
        color: #c1c1cc;
        font-weight: bold;
      }
    }
  }
  &-medium{
    .icon-search{
      font-size: 16px;
      cursor: pointer;
      line-height: 1;
      position: absolute;
      top: 15px;
      right: 5px;
    }
  }
  &-right{
    display: flex;
    .workon-wechat{
      display: flex;
      align-items: center;
      .icon-wechat{
        color: #5DA50E;
        font-size: 22px;
        margin-right: 9px;
      }
    }
    .user-center{
      margin-left: 30px;
      .line{
        width: 1px;
        height: 14px;
        border: $--border-base;
        margin: 0 10px;
      }
    }
  }
}