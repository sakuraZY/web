.menu-manage-container {
  height: 100%;

  .role-tree-container {
    height: 100%;
    width: 100%;
    overflow: auto;
    padding: 10px;
    box-sizing: border-box;
    .role-tree{
      /deep/ {
        .state{
          &-open{
            color: $--color-primary;
          }
          &-close{
            color: $--color-text-secondary;
          }
        }
      }
    }
  }
}